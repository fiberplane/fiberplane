{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": ["ESNext"],
    "esModuleInterop": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"]
    },
    "rootDir": "./src",
    "declaration": true,
    "jsxImportSource": "hono/jsx"
  },
  "include": ["src"]
}
