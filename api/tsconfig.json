{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"lib": [
			"ESNext"
		],
		"types": [
			"@cloudflare/workers-types"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": ".",
		"outDir": "dist",
		"paths": {
			"@/*": [
				"src/*"
			]
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist",
		"scripts"
	]
}
