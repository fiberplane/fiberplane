{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"outDir": "./dist",
		"rootDir": ".",
		"esModuleInterop": true
	},
	"include": ["src/**/*", "migrate.ts"],
	"exclude": ["node_modules"]
}