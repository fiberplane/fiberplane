{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git"
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "space"
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"files": {
		"ignore": ["meta/*.json", "dist/*.js", "test-content/*.*", "fpx/*.*"]
	}
}
